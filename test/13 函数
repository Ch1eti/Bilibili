// 函数
// C语言是【结构化】的程序设计语言  -顺序结构   -选择结构   -顺序结构


#include <stdio.h>

// 函数
int Add(int x, int y)    // int 函数的返回类型   Add 函数名   int x int y 函数参数
{
    int z = 0;     //大括号内部叫  函数体
    z = x + y;
    return z;     //  或者 大括号内部直接写 return (x +y)
}

// 求两个数的和
int main()
{
    int num1 = 0;
    int num2 = 0;
    // 输入
    scanf("%i %i", &num1, &num2);

    // 求和
    // int sum = num1 + num2;
    int sum = Add(num1, num2);

    // 打印
    printf("%i\n", sum);
    return 0;
}
